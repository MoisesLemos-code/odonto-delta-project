{
  "name": "pregao-app",
  "version": "2.0.0",
  "description": "Refatoração do Pregão Legado",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve --port 8051",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "unit-testing": "jest --clearCache && vue-cli-service test:unit --config jest.config.js --no-cache",
    "test:unit:watch": "jest --clearCache && vue-cli-service test:unit --config jest.config.js --no-cache --watchAll"
  },
  "files": [
    "app/**"
  ],
  "repository": "http://git.azi.com.br/hal/pregao/pregao.git",
  "publishConfig": {
    "registry": "http://registry.nexus.azi.srv.br/repository/npm-private/"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@azinformatica/loki": "1.6.0",
    "accounting": "^0.4.1",
    "axios": "^0.21.0",
    "date-fns": "^2.19.0",
    "keycloak-js": "^12.0.1",
    "lodash": "^4.17.20",
    "material-design-icons-iconfont": "^4.0.2",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "node-sass": "^7.0.3",
    "sass": "^1.55.0",
    "sass-loader": "^10",
    "sockjs-client": "1.6.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "v-clipboard": "^2.2.3",
    "validator-brazil": "^1.2.2",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.12",
    "vue-cookies": "^1.7.4",
    "vue-loader": "^17.0.0",
    "vue-router": "^3.4.9",
    "vue-the-mask": "^0.11.1",
    "vuetify": "^2.6.6",
    "vuex": "^3.5.1",
    "webstomp-client": "^1.2.6"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/polyfill": "^7.12.1",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/server-test-utils": "^1.1.1",
    "@vue/test-utils": "^1.1.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.0",
    "cache-loader": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^5.2.3",
    "flush-promises": "^1.0.2",
    "nock": "^10.0.6",
    "prettier": "^1.19.1",
    "sonarqube-scanner": "^2.8.0",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.3.0",
    "webpack": "^4.46.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        4
      ],
      "no-console": "off",
      "max-len": [
        "error",
        {
          "code": 180,
          "tabWidth": 4,
          "ignoreUrls": true
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}