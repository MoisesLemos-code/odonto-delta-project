{
  "name": "odonto-delta-app",
  "version": "1.0.0-ALFA.1",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve --port 8021",
    "start": "node server.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit:watch": "jest --clearCache && vue-cli-service test:unit --config jest.config.js --no-cache --watchAll",
    "unit-testing": "jest --clearCache && vue-cli-service test:unit --config jest.config.js --no-cache"
  },
  "files": [
    "app/**"
  ],
  "dependencies": {
    "@mdi/font": "^5.4.55",
    "accounting": "^0.4.1",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "register-service-worker": "^1.7.1",
    "scrollmonitor": "^1.2.4",
    "validator-brazil": "^1.1.0",
    "vee-validate": "^2.1.3",
    "vue": "^2.6.11",
    "vue-cookies": "^1.5.7",
    "vue-i18n": "^8.18.2",
    "vue-radial-progress": "^0.2.10",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.11",
    "vue-the-mask": "^0.11.1",
    "vue-timers": "^2.0.4",
    "vuetify": "^2.2.4",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-plugin-pwa": "^4.4.6",
    "@vue/cli-plugin-unit-jest": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.2.2",
    "eslint": "^6.2.2",
    "eslint-plugin-vue": "^5.0.0-0",
    "flush-promises": "^1.0.2",
    "material-design-icons-iconfont": "^4.0.5",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vuetify-loader": "^1.3.0",
    "webpack": "^4.44.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "off",
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "semi": [
        2,
        "never"
      ],
      "vue/script-indent": [
        "error",
        4,
        {
          "baseIndent": 1
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
