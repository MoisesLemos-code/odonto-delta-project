[
  {
    "id": "pregao-api",
    "uri": "lb://pregao",
    "predicates": [
      "Path=/pregao/api/**"
    ],
    "filters": [
      "StripPrefix=2"
    ]
  },
  {
    "id": "pregao-app",
    "uri": "http://pregao-app/",
    "predicates": [
      "Path=/pregao/**"
    ],
    "filters": [
      "StripPrefix=1"
    ]
  },
  {
    "id": "pregao-app-bypass",
    "uri": "http://pregao-app/",
    "predicates": [
      "Path=/pregao,false"
    ],
    "filters": [
      "RedirectTo=301,/pregao/"
    ]
  }
]
        